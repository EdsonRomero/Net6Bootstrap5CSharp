<div id="carouselExempleControls" class="carousel slide carousel-dark w-25 mx-auto" data-bs-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="/Images/cafe01.jpeg" class="d-block w-100" alt="café" />
        </div>
        <div class="carousel-item ">
